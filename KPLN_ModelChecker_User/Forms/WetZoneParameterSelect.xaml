<Window x:Class="KPLN_ModelChecker_User.Forms.WetZoneParameterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Выбор строкового параметра" Height="175" Width="450" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="14 8 14 14">
        <TextBlock x:Name="TextBlockRoomCount" Margin="0,0,0,5" Foreground="#FF1A0F42">
        <Run Text="Всего комнат обнаружено: " FontWeight="Bold"/>
        <Run x:Name="RunRoomCount"/>
        </TextBlock>
        <TextBlock Text="Параметр, по которому будет определяться тип помещения:" Margin="0,0,0,5"/>
        <ComboBox x:Name="ComboBoxParameters" IsEnabled="False" Height="25"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,20,0,0">
            <Button Width="80" Height="24" Margin="5,0" Click="Ok_Click">ОК</Button>
            <Button Width="80" Height="24" Margin="5,0" Click="Cancel_Click">Отмена</Button>
        </StackPanel>
    </StackPanel>
</Window>