<Window x:Class="KPLN_Publication.Forms.EnterNameForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KPLN_Publication.Forms"
             mc:Ignorable="d" 
             FontFamily="Monaco, Consolas, 'Andale Mono', 'DejaVu Sans Mono', monospace" ResizeMode="NoResize" SizeToContent="WidthAndHeight" Title="KPLN: Задать имя набора видов" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="5">
            <TextBox x:Name="tb" Margin="5,25,5,0" Text="Новый набор" Width="250" HorizontalAlignment="Left"/>
            <Button Margin="5,5,5,20" HorizontalAlignment="Left" Content="Применить" Background="White" Click="OnApply" IsDefault="True"/>
        </StackPanel>
    </Grid>
</Window>
